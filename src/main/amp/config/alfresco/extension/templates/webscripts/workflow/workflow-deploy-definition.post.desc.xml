<webscript>
	<shortname>Deploy a workflow definition</shortname>
	<description>
		Deploy a workflow definition found at a NodeRef in the repository.
	</description>
	<url>/bees-api/workflow-deploy-definition?workflowDefinitionNodeRef={workflowDefinitionNodeRef}</url>
	<url>/bees-api/workflow-deploy-definition?workflowDefinitionPath={workflowDefinitionPath}</url>
	<format default="json" />
	<authentication>user</authentication>
	<transaction allow="readonly">required</transaction>
	<family>Order Of The Bee</family>
</webscript>